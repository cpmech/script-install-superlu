{
  "name": "ScriptInstallSuperLU",
  "image": "cpmech/script-install-superlu",
  "runArgs": [
    "--cap-add=SYS_PTRACE",
    "--security-opt",
    "seccomp=unconfined"
  ],
  "forwardPorts": [
    9000
  ],
  "remoteUser": "vscode",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.cpptools",
        "twxs.cmake"
      ]
    }
  }
}
